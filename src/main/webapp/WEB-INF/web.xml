    <!--    Register DispatcherServlet-->
    <servlet>
        <servlet-name>dog</servlet-name>
        <servlet-class>
            org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <!--    Map DispatcherServlet to /-->
    <servlet-mapping>
        <servlet-name>dog</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>

